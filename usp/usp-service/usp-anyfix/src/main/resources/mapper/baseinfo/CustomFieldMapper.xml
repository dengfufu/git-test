<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zjft.usp.anyfix.baseinfo.mapper.CustomFieldMapper">

    <select id="selectCustomField" resultType="com.zjft.usp.anyfix.baseinfo.model.CustomField">
        select *
        from custom_field
        where
          (corp_id = #{corpId} and form_type = #{formType})
          or  (corp_id = #{corpId} and common = 'Y' );
    </select>
</mapper>
