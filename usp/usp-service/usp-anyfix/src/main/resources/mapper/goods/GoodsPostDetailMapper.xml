<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zjft.usp.anyfix.goods.mapper.GoodsPostDetailMapper">

    <!--  物品寄送单未签收明细数量  -->
    <select id="findNotSignedCount" resultType="java.lang.Integer">
        select count(id) as id_count from goods_post_d where post_id=#{postId} and signed != 'Y'
    </select>

    <!--  物品寄送单签收明细数量  -->
    <select id="findSignedCount" resultType="java.lang.Integer">
        select count(id) as id_count from goods_post_d where post_id=#{postId} and signed == 'Y'
    </select>
</mapper>
